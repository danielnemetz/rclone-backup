# .env Configuration for Backup Script

# Name of your configured rclone remote (e.g., from 'rclone listremotes')
RCLONE_REMOTE_NAME="my-rclone-remote"

# --- Retention Policy ---
KEEP_DAILY=7
KEEP_WEEKLY=4
KEEP_MONTHLY=6

# --- Optional ---
# Compression level for tar gzip (0-9, default 6 in script)
COMPRESSION_LEVEL=6
